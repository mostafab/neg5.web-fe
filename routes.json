{
  "middleware": [
    "./middleware/request-to-port",
    "./middleware/css-middleware",
    "./middleware/set-logged-in-user"
  ],
  "routes": {
    "UserTournaments": {
      "path": ["/tournaments", "/home"],
      "page": "pages/UserTournamentsPage.js"
    },
    "TournamentInfoPage": {
      "path": ["/tournaments/:tournamentId"],
      "page": "pages/TournamentInfoPage.js"
    },
    "TournamentTeamsPage": {
      "path": ["/tournaments/:tournamentId/teams"],
      "page": "pages/TournamentTeamsPage.js"
    },
    "TournamentMatchesPage": {
      "path": ["/tournaments/:tournamentId/matches"],
      "page": "pages/TournamentMatchesPage.js"
    }
  }
}
